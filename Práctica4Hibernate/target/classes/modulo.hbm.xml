<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="aad.modelo.Modulo" table="Modulo">
   <id name="Id" type="int">
			<column name="ID" />
			<generator class="identity" />
		</id>
  <property column="nombre" generated="never" name="nombre"/>
  <property column="creditos" generated="never" name="creditos"/>
 </class>
</hibernate-mapping>
