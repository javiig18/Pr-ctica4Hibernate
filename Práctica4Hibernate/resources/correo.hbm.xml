<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="aad.modelo.Correo" table="Correo">
   <id name="Id" type="int">
			<column name="ID" />
			<generator class="identity" />
		</id>
  <property column="direccion" generated="never" name="direccion"/>
  <property column="proveedor" generated="never" name="proveedor"/>
 </class>
</hibernate-mapping>
